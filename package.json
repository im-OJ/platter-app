{
  "name": "platter-app",
  "productname": "platter",
  "version": "1.0.2",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "repository": "https://github.com/im-OJ/platter-app.git",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "fetch-schema": "npx get-graphql-schema https://platter-api.herokuapp.com/graphql > src/schema/schema.graphql",
    "generate-types": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "desktop_electron_forge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": ".config/webpack.main.config.js",
            "renderer": {
              "config": ".config/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@graphql-codegen/typescript-document-nodes": "^1.13.2",
    "@graphql-codegen/typescript-resolvers": "^1.13.2",
    "@types/howler": "^2.1.2",
    "@types/lodash": "^4.14.149",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-browser": "^6.1.3",
    "@types/react": "^16.9.10",
    "@types/react-dom": "^16.9.2",
    "@types/react-howler": "^3.7.1",
    "@types/styled-components": "^5.1.0",
    "antd": "^4.1.3",
    "apollo-codegen": "^0.20.2",
    "custom-electron-titlebar": "^3.2.2-hotfix62",
    "electron-squirrel-startup": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^15.0.0",
    "lodash": "^4.17.15",
    "pouchdb": "^7.2.1",
    "pouchdb-browser": "^7.2.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-howler": "^3.7.4",
    "react-player": "^1.15.3",
    "react-use-audio-player": "^0.0.15",
    "reactn": "^2.2.7",
    "styled-components": "^5.1.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "update-electron-app": "^1.5.0",
    "use-sound": "^1.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/maker-deb": "6.0.0-beta.45",
    "@electron-forge/maker-rpm": "6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "6.0.0-beta.45",
    "@electron-forge/maker-zip": "6.0.0-beta.45",
    "@electron-forge/plugin-webpack": "6.0.0-beta.45",
    "@graphql-codegen/cli": "^1.13.2",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@pongsatt/graphql-schema-typescript": "^1.0.25",
    "css-loader": "^3.0.0",
    "electron": "7.0.0",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1"
  }
}
